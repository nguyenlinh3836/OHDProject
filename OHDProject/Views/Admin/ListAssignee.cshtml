@{ Layout = "_AdminLayout"; }
@model IEnumerable<Account>
<style>
    a{color:white}
</style>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Phone</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var assign in Model)
        {
            <tr>
                <td>@assign.AccountId</td>
                <td>@assign.FirstName</td>
                <td>@assign.Contact</td>
                <td>@assign.Email</td>
                <td><a class="btn btn-primary" @assign.AccountId>Assign</a></td>
            </tr>
        }
    </tbody>  
</table>
